{
  "name": "spryker-shop/content-file-widget",
  "description": "ContentFileWidget module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/content-file": "^1.0.0",
    "spryker/file-manager-storage": "^1.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/twig": "^3.5.0",
    "spryker/twig-extension": "^1.0.0",
    "spryker-shop/file-manager-widget": "^1.1.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/container": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "suggest": {
    "spryker-shop/file-manager-widget": "ADD SUGGEST DESCRIPTION",
    "spryker/container": "ADD SUGGEST DESCRIPTION"
  }
}
